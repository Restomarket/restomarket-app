# Test Environment Configuration
NODE_ENV=test
APP_NAME=nestjs-clean-api-test
APP_PORT=3000
APP_HOST=0.0.0.0
API_PREFIX=api
API_VERSION=1
REQUEST_TIMEOUT=30000

# Test Database - Use existing database connection
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/nestjs_clean_db
DATABASE_POOL_MAX=5
DATABASE_IDLE_TIMEOUT=10
DATABASE_CONNECT_TIMEOUT=10
DATABASE_SSL=false

# Logging
LOG_LEVEL=error
LOG_PRETTY=false

# Security
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=1000

# Swagger
SWAGGER_ENABLED=false
SWAGGER_TITLE=NestJS Clean API Test
SWAGGER_DESCRIPTION=Test Environment
SWAGGER_VERSION=1.0
