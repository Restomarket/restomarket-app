# Docker Compose Local Development Environment Variables
# Copy this file to .env.development and adjust values as needed

# Application
NODE_ENV=development
API_PORT=3000
LOG_LEVEL=debug

# PostgreSQL Database
POSTGRES_DB=restomarket
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_PORT=5432

# Redis Cache
REDIS_PASSWORD=redis
REDIS_PORT=6379

# API Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
SWAGGER_ENABLED=true

# Adminer
ADMINER_PORT=8080

# Database URL (for API container)
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/restomarket
